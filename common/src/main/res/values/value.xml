    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <declare-styleable name="AdjustTextView"><attr format="dimension" name="oscMinTextSize"/></declare-styleable>
    <declare-styleable name="FlowLayout"><attr format="dimension" name="oscVerticalSpace"/><attr format="dimension" name="oscHorizontalSpace"/></declare-styleable>
